{
  "name": "mustufa/laravel-payfort",
  "description": "Laravel-payfort is a simple package to process payments throught Payfort payment gateway.",
  "keywords": [
    "http",
    "payment",
    "payfort",
    "laravel payfort"
  ],
  "authors": [
    {
      "name": "Mustufa Vohra",
      "email": "mustafa.vhora@hotmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/wshurafa/laravel-payfort/issues",
    "source": "https://github.com/wshurafa/laravel-payfort"
  },
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0.1",
    "illuminate/support": "*",
    "nesbot/carbon": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "fabpot/goutte": "*"
  },
  "autoload": {
    "psr-4": {
      "LaravelPayfort\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelPayfort\\Providers\\PayfortServiceProvider"
      ],
      "aliases": {
        "Payfort": "LaravelPayfort\\Facades\\Payfort"
      }
    }
  }
}
